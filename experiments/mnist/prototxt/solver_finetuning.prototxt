net: "finetuning_mnist.prototxt"
type: "SGD"

test_state: { stage: 'test-on-train' }
test_iter: 500
test_interval: 3000

base_lr: 0.000001
momentum: 0.9
weight_decay: 0.0005

# The learning rate policy
lr_policy: "fixed"
#gamma: 0.5

# Display every 100 iterations
display: 100
max_iter: 4000


snapshot: 1000
snapshot_prefix: "../snapshots/snapshot_finetuning_mnist"


# solver mode: CPU or GPU
solver_mode: CPU
