net: "finetuning_mnist.prototxt"
type: "SGD"

#test_state: { stage: 'test-on-train' }
#test_iter: 500
#test_state: { stage: 'test-on-test' }
test_iter: 500
test_interval: 3000

base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005

# The learning rate policy
lr_policy: "fixed"

# Display every 100 iterations
display: 100
max_iter: 40000


snapshot: 4000
snapshot_prefix: "../../snapshots/mnist_standar_finetuning_10000_lr0.01"

# solver mode: CPU or GPU
solver_mode: GPU
